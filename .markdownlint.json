{
  "extends": "markdownlint/style/relaxed",
  "line-length": false,
  "no-hard-tabs": true,
  "no-trailing-spaces": true,
  "no-multiple-blanks": {
    "maximum": 3
  },
  "list-marker-space": true,
  "blanks-around-lists": true,
  "blanks-around-headings": true,
  "no-duplicate-heading": false,
  "heading-style": false,
  "single-title": false,
  "fenced-code-language": false,
  "no-emphasis-as-heading": false,
  "ol-prefix": false,
  "no-alt-text": false,
  "code-block-style": false,
  "ul-style": false,
  "no-empty-links": false,
  "hr-style": false,
  "table-pipe-alignment": false,
  "table-column-alignment": false,
  "table-columns": false,
  "md060": false,
  "md056": false,
  "md051": false,
  "required-headings": false,
  "required-heading-increment": false,
  "proper-names": false,
  "no-inline-html": false,
  "no-bare-urls": false
}
