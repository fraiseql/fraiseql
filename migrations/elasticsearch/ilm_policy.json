{
  "policy": "fraiseql-events-policy",
  "phases": {
    "hot": {
      "min_age": "0ms",
      "actions": {
        "rollover": {
          "max_age": "7d",
          "max_size": "50gb"
        }
      }
    },
    "warm": {
      "min_age": "30d",
      "actions": {
        "shrink": {
          "number_of_shards": 1
        },
        "readonly": {}
      }
    },
    "delete": {
      "min_age": "90d",
      "actions": {
        "delete": {}
      }
    }
  }
}
