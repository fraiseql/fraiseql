{
  "name": "fraiseql-nodejs",
  "version": "1.0.0",
  "description": "FraiseQL Node.js authoring language - 100% feature parity with Python, TypeScript, Java, and Go",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rm -rf dist coverage"
  },
  "keywords": [
    "fraiseql",
    "graphql",
    "authorization",
    "security",
    "rbac",
    "abac",
    "schema"
  ],
  "author": "FraiseQL Contributors",
  "license": "Apache-2.0",
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
